{
  "name": "saka-msg test extension",
  "version": "0.0",
  "description": "Tests and Demonstrates saka-msg",
  "background": {
    "scripts": ["background_page.js"]
  },
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["content_script.js"]
    }
  ],
  "browser_action": {
    "default_title": "saka-msg",
    "default_icon": "saka.png",
    "default_popup": "popup.html"
  },
  "manifest_version": 2
}
